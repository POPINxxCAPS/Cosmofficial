<div class="container">
    <div class="row">
        <div class="col offset-md-1">
            <div class="btn-toolbar">
                <div class="btn-group">
                    <a href="https://cosmofficial.herokuapp.com/" class="btn btn-success">Home Page</a>
                    <a href="/commandList" class="btn btn-secondary">Command List</a>
                    <a href="https://discord.gg/BfFc8cfp3n" class="btn btn-secondary">Cosmofficial Discord</a>
                    <a href="https://discord.com/api/oauth2/authorize?client_id=845784381025157131&permissions=8&scope=bot"
                        class="btn btn-primary">Bot Invite Link</a>
                    <a href="https://patreon.com/Cosmofficial" class="btn btn-secondary">Patreon</a>
                    <a href="/serverList" class="btn btn-secondary">Server List</a>
                    <% if(reqObj.authURL === "Logged-In") { %>
                    <a href="/profile/<%= reqObj.user._username %>" class="btn btn-success">Your Servers</a>
                    <a href="/signOut" class="btn btn-danger">Sign-Out</a>
                    <% } else { %>
                    <a href="<%= reqObj.authURL %>" class="btn btn-success">Log-In</a>
                    <% } %>
                </div>
            </div>
        </div>
    </div>
</div>